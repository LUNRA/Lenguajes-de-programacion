PROGRAMA -> SUBPROCESO PROCESO SUBPROCESO

SUBPROCESO -> INICIOSUBPROCESO VARIABLE_RETORNO id SUMPROCESO_ARG CUERPO FINSUBPROCESO SUBPROCESO 

SUMPROCESO_ARG -> token_par_izq ARGUMENTO token_par_der
SUBPROCESO_ARG -> epsilon

INICIOSUBPROCESO -> subproceso
INICIOSUBPROCESO -> subalgoritmo
INICIOSUBPROCESO -> funcion
FINSUBPROCESO -> finsubproceso
FINSUBPROCESO -> finsubalgoritmo
FINSUBPROCESO -> finfuncion

SUBPROCESO -> epsilon

VARIABLE_RETORNO -> id <- 
VARIABLE_RETORNO -> epsilon

ARGUMENTO -> epsilon
ARGUMENTO -> id LISTA

LISTA -> token_coma id LISTA
LISTA -> epsilon

PROCESO -> INICIOPROCESO id CUERPO FINPROCESO
INICIOPROCESO -> proceso
INICIOPROCESO -> algoritmo
FINPROCESO -> finproceso
FINPROCESO -> finalgoritmo

CUERPO -> DEFINICION CUERPO
CUERPO -> ASIGNACION CUERPO
CUERPO -> INSTRUCCION CUERPO
CUERPO -> DIMENSION CUERPO
CUERPO -> ACCEDER_ARREGLO CUERPO
CUERPO -> epsilon


DIMENSION -> dimension ARREGLO LISTA_ARREGLOS token_pyc

ARREGLO-> id DIM
DIM -> token_cor_izq EXPRESION LISTA_DIM token_cor_der
DIM -> token_par_izq EXPRESION LISTA_DIM token_par_der
LISTA_DIM -> token_coma EXPRESION LISTA_DIM
LISTA_DIM -> epsilon

LISTA_ARREGLOS -> token_coma ARREGLO LISTA_ARREGLOS
LISTA_ARREGLOS -> epsilon

ACCEDER_ARREGLO -> id INDEX token_pyc
INDEX -> token_par_izq LISTA_DIM token_par_der
INDEX -> token_cor_izq LISTA_DIM token_cor_der

DEFINICION -> definir id LISTA como TIPO token_pyc 

ASIGNACION -> id token_asig ASGINAR token_pyc 

ASIGNAR -> EXPRESION
ASGINAR -> LLAMADO_PROCESO 

LLAMADO_PROCESO -> id token_par_izq ARG_LLAMADO token_par_der
ARG_LLAMADO -> ARGUMENTO 
ARG_LLAMADO -> LISTA_EXPR 

LISTA_EXPR -> EXPRESION token_coma LISTA_EXPR
LISTA_EXPR -> epsilon

INICIO_LLAMADO_PROCESO -> id token_par_izq  

EXPRESION -> TERMINO COMPLEMENTO

COMPLEMENTO -> token_y TERMINO COMPLEMENTO
COMPLEMENTO -> token_o TERMINO COMPLEMENTO
COMPLEMENTO -> EXPRESION_LOGICA
//seria complemento, no complemento logico
EXPRESION_LOGICA -> token_igual TERMINO COMPLEMENTO_LOGICO
EXPRESION_LOGICA -> token_dif TERMINO COMPLEMENTO_LOGICO
EXPRESION_LOGICA -> token_menor TERMINO COMPLEMENTO_LOGICO
EXPRESION_LOGICA -> token_mayor TERMINO COMPLEMENTO_LOGICO
EXPRESION_LOGICA -> token_menor_igual TERMINO COMPLEMENTO_LOGICO
EXPRESION_LOGICA -> token_mayor_igual TERMINO COMPLEMENTO_LOGICO
EXPRESION_LOGICA -> EXPRESION_MAT

COMPLEMENTOS_ADICIONALES -> token_y TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> token_o TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> token_mas TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> token_menos TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> token_mul TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> token_mod TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> token_div TERMINO COMPLEMENTO
COMPLEMENTOS_ADICIONALES -> epsilon

EXPRESION_MAT -> token_mas TERMINO COMPLEMENTO
EXPRESION_MAT -> token_menos TERMINO COMPLEMENTO
EXPRESION_MAT -> epsilon
EXPRESION_MAT -> EXPRESION_MAT2

EXPRESION_MAT2 -> token_mul TERMINO COMPLEMENTO
EXPRESION_MAT2 -> token_mod TERMINO COMPLEMENTO
EXPRESION_MAT2 -> token_div TERMINO COMPLEMENTO
EXPRESION_MAT2 -> epsilon

TERMINO -> token_par_izq EXPRESION token_par_der
TERMINO -> CONSTANTE
TERMINO -> token_menos CONSTANTE
TERMINO -> token_mas COSNTANTE
TERMINO -> token_neg CONSTANTE

CONSTANTE -> token_entero
CONSTANTE -> token_real
CONSTANTE -> token_caracter
CONSTANTE -> token_cadena
CONSTANTE -> falso
CONSTANTE -> verdadero

TIPO -> caracter
TIPO -> entero
TIPO -> logico
TIPO -> numero
TIPO -> numerico
TIPO -> real
TIPO -> texto
TIPO -> cadena

INSTRUCCION -> esperar INS_ESPERAR token_pyc
INSTRUCCION -> borrar pantalla token_pyc 
INSTRUCCION -> escribir POR_ESCRIBIR token_pyc 

INS_ESPERAR -> tecla 
INS_ESPERAR -> ESPERAR_NUMERO segundos  

ESPERAR_NUMERO -> token_entero
ESPERAR_NUMERO -> token_real
ESPERAR_NUMERO -> id


POR_ESCRIBIR -> TERMINO LISTA_ESCRIBIR
POR_ESCRIBIR -> EXPRESION LISTA_ESCRIBIR
POR_ESCRIBIR -> id LISTA_ESCRIBIR
POR_ESCRIBIR -> LLAMADO_PROCESO LISTA_ESCRIBIR

LISTA_ESCRIBIR -> token_coma TERMINO LISTA_ESCRIBIR
LISTA_ESCRIBIR -> token_coma EXPRESION LISTA_ESCRIBIR
LISTA_ESCRIBIR -> token_coma id LISTA_ESCRIBIR
LISTA_ESCRIBIR -> LLAMADO_PROCESO id LISTA_ESCRIBIR
LISTA_ESCRIBIR -> epsilon


CONDICIONAL_SI -> si  EVALUAR_PAR entonces CUERPO SI_NO finsi

EVALUAR_PAR -> token_par_izq EVALUAR token_par_der
EVALUAR_PAR -> EVALUAR 

EVALUAR -> verdadero
EVALUAR -> falso
EVALUAR -> TERMINO_SI COMPARATIVO 

TERMINO_SI -> token_par_izq EXPRESION token_par_der
TERMINO_SI -> CONSTANTE_SI
TERMINO_SI -> token_menos CONSTANTE_SI
TERMINO_SI -> token_mas COSNTANTE_SI
TERMINO_SI -> token_neg CONSTANTE_SI

CONSTANTE_SI -> token_entero
CONSTANTE_SI -> token_real
CONSTANTE_SI -> token_caracter
CONSTANTE_SI -> token_cadena
CONSTANTE_SI -> falso
CONSTANTE_SI -> verdadero
CONSTANTE_SI -> id

COMPARATIVO -> token_igual TERMINO_SI
COMPARATIVO -> token_dif TERMINO_SI
COMPARATIVO -> token_mayor TERMINO_SI
COMPARATIVO -> token_menor TERMINO_SI
COMPARATIVO -> token_mayor_igual TERMINO_SI
COMPARATIVO -> token_menor_igual TERMINO_SI

SI_NO -> sino CUERPO
SI_NO -> epsilon

CICLO_PARA -> para id token_asig EXPRESION_PARA hasta EXPRESION_PARA CON_PASO hacer CUERPO finpara

EXPRESION_PARA -> token_par_izq PARA_VALIDO token_par_der
EXPRESION_PARA -> PARA_VALIDO

PARA_VALIDO ->  token_entero OPERACION
PARA_VALIDO ->  token_real OPERACION
PARA_VALIDO ->  id OPERACION

OPERACION -> token_mas PARA_VALIDO
OPERACION -> token_menos PARA_VALIDO
OPERACION -> token_mul PARA_VALIDO
OPERACION -> token_div PARA_VALIDO
OPERACION -> token_mod PARA_VALIDO
OPERACION -> epsilon

CON_PASO -> PARA_VALIDO
CON_PASO -> epsilon

CICLO_MIENTRAS -> mientras EVALUAR_PAR hacer CUERPO finmientras

CICLO_REPETIR -> repetir CUERPO hasta que EVALUAR_PAR